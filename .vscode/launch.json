{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "GDB Linux Native",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceRoot}/build/swegui",
            "args": [ ],
            "stopAtEntry": false,
            "cwd": "${workspaceRoot}/build",
            "environment": [],
            "externalConsole": true,
            "MIMode": "gdb",
            "miDebuggerPath": "/usr/bin/gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
