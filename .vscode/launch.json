{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Attach GDB Linux Native",
            "type": "cppdbg",
            "request": "attach",
            "program": "${workspaceRoot}/build/build_swegui/swegui",
            "processId": "${command:pickProcess}",
            "linux": {
                "MIMode": "gdb",
                "miDebuggerPath": "/usr/bin/gdb",
                "setupCommands": [
                    {
                        "description": "Enable pretty-printing for gdb",
                        "text": "-enable-pretty-printing",
                        "ignoreFailures": true
                    }
                ]
            }
        },
        {
            "name": "Attach GDB WSL",
            "type": "cppdbg",
            "request": "attach",
            "processId": "14307",
            //"processId": "${command:pickRemoteProcess}",
            "program": "/mnt/c/Users/Christoph/Documents/Studium/5_WS1718/TSISIM/Tutorium/SWEGUI/build/packet/swegui",
            "windows": {
                "MIMode": "gdb",
                "setupCommands": [
                    {
                        "description": "Enable pretty-printing for gdb",
                        "text": "-enable-pretty-printing",
                        "ignoreFailures": true
                    }
                ]
            }, 
            "pipeTransport": {
                "pipeCwd": "",
                "pipeProgram": "c:/windows/sysnative/bash.exe",
                "pipeArgs": [ "-c" ],
                "debuggerPath": "/usr/bin/gdb"
            },
            "sourceFileMap": {
                "/usr": "c:/lxss_ro/rootfs/usr",
                "/mnt/c": "c:/"
            }
        },
        {
            "name": "Launch GDB WSL",
            //"logging" : { "trace": true, "traceResponse": true, "engineLogging": true },
            "type": "cppdbg",
            "request": "launch",
            "program": "/mnt/c/Users/Christoph/Documents/Studium/5_WS1718/TSISIM/Tutorium/SWEGUI/build/packet/swegui",
            "args": [ 
                "--display=127.0.0.1:0"
            ],
            "stopAtEntry": false,
            "cwd": "/mnt/c/Users/Christoph/Documents/Studium/5_WS1718/TSISIM/Tutorium/SWEGUI/build/packet",
            "environment": [],
            "externalConsole": true,
            "windows": {
                "MIMode": "gdb",
                "setupCommands": [
                    {
                        "description": "Enable pretty-printing for gdb",
                        "text": "-enable-pretty-printing",
                        "ignoreFailures": true
                    }
                ]
            }, 
            "pipeTransport": {
                "pipeCwd": "",
                "pipeProgram": "c:/windows/sysnative/bash.exe",
                "pipeArgs": ["-c"],
                "debuggerPath": "/usr/bin/gdb"
            },
            "sourceFileMap": {
                "/usr": "c:/lxss_ro/rootfs/usr",
                "/mnt/c": "c:/"
            }
        },
